{
  "name": "uistudio-core",
  "version": "0.0.1",
  "description": "A collection of unstyled and keyboard friendly UI components for your React App.",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test-switch": "yarn workspace @uistudio/switch test --coverage",
    "build-switch": "yarn workspace @uistudio/switch build",
    "storybook-switch": "yarn workspace @uistudio/switch storybook",
    "test-treeview": "yarn workspace @uistudio/treeview test --coverage",
    "build-treeview": "yarn workspace @uistudio/treeview build",
    "storybook-treeview": "yarn workspace @uistudio/treeview storybook",
    "buildPackages": "yarn build-switch & yarn build-treeview",
    "testPackages": "yarn test-switch & yarn test-treeview",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "build-storybook-docs": "build-storybook --docs",
    "npm-publish": "yarn workspace @uistudio/switch publish && yarn workspace @uistudio/treeview publish"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^8.2.5",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "rollup": "^2.77.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.7"
  },
  "keywords": [
    "react",
    "ui",
    "switch",
    "treeview",
    "accessibility"
  ]
}
